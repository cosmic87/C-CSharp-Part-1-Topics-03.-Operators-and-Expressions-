using System;
class Program
    {
        static void Main(string[] args)
        {
        int a = int.Parse(Console.ReadLine());
        if ((a % 7 == 0) && (a % 5 == 0))
        {
            Console.WriteLine("true {0}", a);
         }
        else
        {
            Console.WriteLine("false {0}", a);
        }
        }
    }
