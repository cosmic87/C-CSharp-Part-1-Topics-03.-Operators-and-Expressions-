using System;
class Program
    {
        static void Main()
        {
        double x = double.Parse(Console.ReadLine());
        double y = double.Parse(Console.ReadLine());
        double r = 1.5;
        bool ispointcircle, isoutsiderectangle;
        ispointcircle = ((x - 01) * 2 + (y - 1) * 2) <= r * r;
        isoutsiderectangle = (x < -1 || x > 5) || (y < -1 || y > 1);
        if(ispointcircle)
            {
            Console.WriteLine("inside circle");
            }
        else
        {
            Console.WriteLine("ouside circle");
        }
        if(isoutsiderectangle)
        {
            Console.WriteLine("outside rectangle");
        }
        else
        {
            Console.WriteLine("inside rectangle");
        }
    }
    }
